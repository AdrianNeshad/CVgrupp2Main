@model ErfarenheterViewModel



@{
    ViewData["Title"] = "Lägg till Erfarenhet";
}

<h3>Lägg till erfarenheter</h3>

<form asp-action="LäggTillErfarenhet" method="post">
    <div asp-validation-summary="All" class="text-danger"></div>

    <input type="hidden" asp-for="ID" />

    <div class="form-group">
        <label asp-for="Titel">Titel</label>
        <input asp-for="Titel" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Beskrivning">Beskrivning</label>
        <textarea asp-for="Beskrivning" class="form-control"></textarea>
    </div>

    <button type="submit" class="btn btn-primary">Lägg till erfarenhet</button>
</form>

